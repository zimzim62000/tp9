
{% extends 'base.html.twig' %}


{% block body %}

    <h2>index skin</h2>

    <ul>
        {% for skin in skins %}
            <li>
                {{ skin.name }} - {{ skin.price }} - {{ skin.createdAt |date("d/m/Y") }} - {{ skin.user.email }} -<a href="{{ path('app_skin_index', {'id': skin.id}) }}">Show</a>
            </li>
        {% endfor %}
    </ul>



{% endblock %}